<ExtensionImage xmlns="http://schemas.microsoft.com/windowsazure" xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
  <ProviderNameSpace><%= chef_namespace %></ProviderNameSpace>
  <Type><%= extension_name %></Type>
  <!--Update this version for each new release-->
  <Version><%= extension_version %></Version>
  <Label>Chef Extension for <%= target_type %></Label>
  <HostingResources>VmRole</HostingResources>
  <!--Update this field with correct link where the extension is published.-->
  <MediaLink>https://<%= package_storage_account %>.blob.core.<%= cloud_platform %>.net/<%= package_container %>/<%= package_name %></MediaLink>
  <Description>Chef Extension that sets up chef-client on VM</Description>
  <LocalResources />
  <IsInternalExtension><%= is_internal %></IsInternalExtension>
  <Eula>http://www.chef.io/about/</Eula>
  <PrivacyUri>http://www.chef.io/about/</PrivacyUri>
  <HomepageUri>http://www.chef.io/</HomepageUri>
  <IsJsonExtension>true</IsJsonExtension>
  <CompanyName>Chef Software, Inc.</CompanyName>
 </ExtensionImage>
